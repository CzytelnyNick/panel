{% extends 'base.html.twig' %}

{% block body %}
    <table class="table table-dark">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Email</th>

        </tr>
        </thead>
        <tbody>

            {% for user in users %}
            <tr class="table-light">
            <th scope="row">{{ user.id }}</th>
            <td>{{ user.email }}</td>

            </tr>
                {% endfor %}


        </tbody>
    </table>




{% endblock %}